{
  "name": "@onigoetz/stream-plumber",
  "version": "1.0.0",
  "description": "Prevent pipe breaking caused by errors from gulp plugins",
  "keywords": [
    "gulpplugin",
    "stream"
  ],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/onigoetz/stream-plumber.git"
  },
  "scripts": {
    "format": "biome format index.js test",
    "check": "biome check index.js test",
    "test": "ava 'test/*\\.test\\.js'",
    "test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=index.js ava 'test/*\\.test\\.js'"
  },
  "dependencies": {
    "ansi-colors": "^4.0.0",
    "fancy-log": "2.0.0",
    "plugin-error": "2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "ava": "6.1.3",
    "c8": "9.1.0",
    "gulp": "^5.0.0",
    "streamx": "2.16.1",
    "through2": "4.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "private": true
}
