{
	"name": "@onigoetz/stream-plumber",
	"version": "1.1.0",
	"description": "Prevent pipe breaking caused by errors from gulp plugins",
	"keywords": [
		"gulpplugin",
		"stream"
	],
	"main": "./index.js",
	"repository": {
		"type": "git",
		"url": "git://github.com/onigoetz/stream-plumber.git"
	},
	"scripts": {
		"format": "biome format .",
		"check": "biome check .",
		"test": "ava 'test/*\\.test\\.js'",
		"test:ci": "c8 --clean --reporter=text --reporter=lcov --reporter=html --include=index.js ava 'test/*\\.test\\.js'"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.4",
		"ava": "6.4.0",
		"c8": "10.1.3",
		"gulp": "^5.0.0",
		"stream-composer": "1.0.2",
		"streamx": "2.22.1",
		"stromjs": "0.5.5",
		"through2": "4.0.2"
	},
	"engines": {
		"node": ">=18"
	},
	"license": "MIT",
	"packageManager": "yarn@4.9.2"
}
